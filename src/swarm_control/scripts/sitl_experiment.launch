<launch>
	<!-- 无人机集群数量 -->
	<arg name="swarm_num_uav" default="9"/>

	<!-- swarm_estimator 参数 -->
	<arg name="input_source" default="2"/>
	<!-- swarm_controller 参数 -->
	<arg name="collision_flag" default="0"/>
	<arg name="Takeoff_height" default="1.0"/>
	<arg name="Disarm_height" default="0.2"/>
	<arg name="Land_speed" default="0.2"/>
	<arg name="Land_mode" default="1"/>
<!-- 无人机uav_id -->
	<arg name="uav1_id" default="1"/>
	<arg name="uav2_id" default="2"/>
	<arg name="uav3_id" default="3"/>
	<arg name="uav4_id" default="4"/>
	<arg name="uav5_id" default="5"/>
	<arg name="uav6_id" default="6"/>
	<arg name="uav7_id" default="7"/>
	<arg name="uav8_id" default="8"/>
	<arg name="uav9_id" default="9"/>
<!-- 无人机初始位置 -->
	<arg name="uav1_init_x" default="0"/>
    <arg name="uav1_init_y" default="-3"/>

	<arg name="uav2_init_x" default="6"/>
    <arg name="uav2_init_y" default="-3"/>

	<arg name="uav3_init_x" default="-6"/>
    <arg name="uav3_init_y" default="-3"/>

	<arg name="uav4_init_x" default="0"/>
    <arg name="uav4_init_y" default="3"/>

	<arg name="uav5_init_x" default="6"/>
    <arg name="uav5_init_y" default="3"/>

	<arg name="uav6_init_x" default="-6"/>
    <arg name="uav6_init_y" default="3"/>

	<arg name="uav7_init_x" default="0"/>
    <arg name="uav7_init_y" default="9"/>

	<arg name="uav8_init_x" default="6"/>
    <arg name="uav8_init_y" default="9"/>

	<arg name="uav9_init_x" default="-6"/>
    <arg name="uav9_init_y" default="9"/>

<!-- 全局变量 -->
	<!-- 地理围栏 -->
	<arg name="x_min" default="-100.0"/>
	<arg name="x_max" default="100.0"/>
	<arg name="y_min" default="-50.0"/>
	<arg name="y_max" default="50.0"/>
	<arg name="z_min" default="-1.0"/>
	<arg name="z_max" default="50.0"/>

	<param name="experiment_begin_time" value="0"/>

	<param name="sensor_noise_stdev" value="0.01"/>

	<param name="sensor_range" value="10"/>
	<!-- site相关信息,global param -->
	<param name="site_number" value="3"/>
	<param name="site_radius" value="10"/>


	<param name="site_posx_1" value="-30"/>
	<param name="site_posy_1" value="10"/>
	<param name="site_posz_1" value="0"/>
	<param name="site_quality_1" value="0.4"/>

	<param name="site_posx_2" value="30"/>
	<param name="site_posy_2" value="30"/>
	<param name="site_posz_2" value="0"/>
	<param name="site_quality_2" value="0.6"/>

	<param name="site_posx_3" value="10"/>
	<param name="site_posy_3" value="-20"/>
	<param name="site_posz_3" value="0"/>
	<param name="site_quality_3" value="0.8"/>
<!-- 启动SITL -->
    <!-- 1号飞机 -->
	<include file="$(find prometheus_gazebo)/launch_swarm/sitl_swarm_basic.launch">
		<arg name="swarm_num_uav" value="$(arg swarm_num_uav)"/>
		<arg name="uav_id" value="$(arg uav1_id)"/>
		<arg name="sdf" value="$(find prometheus_gazebo)/amov_models/Multisolo/uav1/uav1.sdf"/>
		<arg name="model" value="uav1"/>
		<arg name="uav_init_x" value="$(arg uav1_init_x)"/>
		<arg name="uav_init_y" value="$(arg uav1_init_y)"/>
		<arg name="uav_init_z" value="0.15"/>
		<arg name="uav_init_yaw" value="0.0"/>
	</include>
    <!-- 2号飞机 -->
	<include file="$(find prometheus_gazebo)/launch_swarm/sitl_swarm_basic.launch">
		<arg name="swarm_num_uav" value="$(arg swarm_num_uav)"/>
		<arg name="uav_id" value="$(arg uav2_id)"/>
		<arg name="sdf" value="$(find prometheus_gazebo)/amov_models/Multisolo/uav2/uav2.sdf"/>
